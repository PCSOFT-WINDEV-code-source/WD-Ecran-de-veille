     code : |1-
      
      gnHwnd est un entier systeme
      
      // Récupère le handle passé en paramètre
      gnHwnd = Val(SansEspace(ExtraitChaîne(Majuscule(LigneCommande()), 2, " ")))
      
      vcRect est composée de 
      	nLeft   est un entier
      	nTop    est un entier
      	nRight  est un entier
      	nBottom est un entier
      FIN
      
      // Récupère les dimensions de la fenêtre de prévisualisation de Windows
      API("user32", "GetClientRect", gnHwnd, &vcRect)
      
      // Redimensionne la fenêtre
      MaFenêtre..Largeur = vcRect.nRight
      MaFenêtre..Hauteur = vcRect.nBottom	
      
      // Indique que notre fenêtre devient une fille de la fenêtre de prévisualisation de Windows
      api("user32", "SetParent", handle(), gnHwnd)
      
      // La fenêtre de prévisualisation de Windows reprend le focus
      SysDonneFocus(gnHwnd)
